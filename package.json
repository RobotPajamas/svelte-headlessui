{
  "name": "@robotpajamas/svelte-headlessui",
  "description": "A somewhat complete, mostly full-featured, unofficial Svelte port of Headless UI, an unstyled, fully accessible UI component library.",
  "keywords": [
    "svelte",
    "headlessui",
    "svelte3",
    "svelte-components",
    "headless",
    "sveltejs"
  ],
  "homepage": "https://svelte-headlessui.goss.io",
  "bugs": {
    "url": "https://github.com/robotpajamas/svelte-headlessui/issues"
  },
  "license": "MIT",
  "repository": "github:robotpajamas/svelte-headlessui",
  "version": "1.0.2",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --ignore-path .gitignore --plugin-search-dir . --write .",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir . . && eslint --ignore-path .gitignore .",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "preview": "vite preview",
    "test": "jest"
  },
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@robotpajamas/svelte-heroicons": "^2.0.15-rc.3",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.15.5",
    "@sveltejs/package": "^2.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^29.5.0",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "publint": "^0.1.11",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-inline-compile": "^0.2.0",
    "svelte-jester": "^2.3.2",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^5.0.3",
    "svelte2tsx": "^0.6.11",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "type": "module"
}
