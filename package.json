{
  "name": "@robotpajamas/svelte-headlessui",
  "version": "1.1.0-rc0",
  "description": "A somewhat complete, mostly full-featured, unofficial Svelte port of Headless UI, an unstyled, fully accessible UI component library.",
  "homepage": "https://svelte-headlessui.goss.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/robotpajamas/svelte-headlessui"
  },
  "author": {
    "name": "SJ",
    "email": "suresh@robotpajamas.com",
    "url": "https://sureshjoshi.com"
  },
  "bugs": {
    "url": "https://github.com/robotpajamas/svelte-headlessui/issues"
  },
  "license": "MIT",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "svelte",
    "headlessui",
    "svelte3",
    "svelte-components",
    "headless",
    "sveltejs"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --ignore-path .gitignore --plugin-search-dir . --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "package": "svelte-kit sync && svelte-package && publint",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "svelte": "^3.58.0"
  },
  "devDependencies": {
    "@robotpajamas/svelte-heroicons": "^2.0.15-rc.3",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.15.5",
    "@sveltejs/package": "^2.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "happy-dom": "^9.5.0",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "publint": "^0.1.11",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^5.0.3",
    "svelte2tsx": "^0.6.11",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  }
}
